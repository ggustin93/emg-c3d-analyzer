{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "s-maxage=0"
      },
      "dest": "/frontend/$1"
    }
  ],
  "buildCommand": "cd frontend && npm i && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm i",
  "framework": null
}